

#include <bits/stdc++.h>

using namespace std;

class Node{
    public:
    int data;
    Node* next;
    
    Node(int data){
        this->data=data;
        this->next=NULL;
    }
};

void insertATtail(Node* &head,int d){
    Node* temp=new Node(d);
   temp->next=head;
  head=temp;
}

void print(Node* &head){
    Node* temp=head;
    while(temp!=NULL){
        cout<<temp->data<<" ";
        temp=temp->next;
    }
}
bool find(vector<int>arr){
    int n=arr.size();
    int s=0,e=n-1;
    while(s<=e){
        if(arr[s++]!=arr[e--]){
            return 0;
        }
    }
    return 1;
}
void palindrome(Node* head){
    vector<int>v;
    Node* temp=head;
    while(temp!=NULL){
        v.push_back(temp->data);
    }
    bool check=find(v);
    cout<<check<<endl;
}

int main()
{
   Node* node1=new Node(12);

Node* head=node1;
insertATtail(head,11);

insertATtail(head,12);
insertATtail(head,10);
insertATtail(head,9);
print(head);
cout<<endl;



palindrome(head);
    return 0;
}
